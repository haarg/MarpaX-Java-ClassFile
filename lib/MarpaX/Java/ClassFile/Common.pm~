package MarpaX::Java::ClassFile:Common;
use Data::Section -setup;

sub bnf_top {
  
}

1;
__DATA__
__[ top ]__
:default ::= action => [values]
lexeme default = action => [ start, length, value ] latm => 1
inaccessible is ok by default

__[ bottom ]__
########################################
#           Common rules               #
########################################
u1 ::= U1                 action => MarpaX::Java::ClassFile:Common::u1
u2 ::= U2                 action => MarpaX::Java::ClassFile:Common::u2
u4 ::= U4                 action => MarpaX::Java::ClassFile:Common::u4

# ----------------
# Internal Lexemes
# ----------------
_U1      ~ [\s\S]
_U2      ~ _U1 _U1
_U4      ~ _U2 _U2
_MANAGED ~ [^\s\S]

########################################
#          Common lexemes              #
########################################
U1 ~ _U1
U2 ~ _U2
U4 ~ _U4
