#!perl
use strict;
use warnings FATAL => 'all';
use Data::Scan::Printer;
use MarpaX::Java::ClassFile;
use POSIX qw/EXIT_SUCCESS EXIT_FAILURE/;

# PODNAME: classdump

# ABSTRACT: java .class dump

# VERSION

# AUTHORITY

autoflush STDOUT 1;

map { dspp(MarpaX::Java::ClassFile->new(filename => $_)->ast ) } @ARGV;

exit(EXIT_SUCCESS);
